{
  "name": "@blocknote/core",
  "homepage": "https://github.com/TypeCellOS/BlockNote",
  "private": false,
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TypeCellOS/BlockNote.git",
    "directory": "packages/core"
  },
  "license": "MPL-2.0",
  "version": "0.35.0",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "keywords": [
    "react",
    "javascript",
    "editor",
    "typescript",
    "prosemirror",
    "wysiwyg",
    "rich-text-editor",
    "notion",
    "yjs",
    "block-based",
    "tiptap"
  ],
  "description": "A \"Notion-style\" block-based extensible text editor built on top of Prosemirror and Tiptap.",
  "type": "module",
  "source": "src/index.ts",
  "types": "./types/src/index.d.ts",
  "main": "./dist/blocknote.cjs",
  "module": "./dist/blocknote.js",
  "exports": {
    ".": {
      "types": "./types/src/index.d.ts",
      "import": "./dist/blocknote.js",
      "require": "./dist/blocknote.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "style": "./dist/style.css"
    },
    "./fonts/inter.css": {
      "import": "./src/fonts/inter.css",
      "require": "./src/fonts/inter.css",
      "style": "./src/fonts/inter.css"
    },
    "./comments": {
      "types": "./types/src/comments/index.d.ts",
      "import": "./dist/comments.js",
      "require": "./dist/comments.cjs"
    },
    "./locales": {
      "types": "./types/src/i18n/index.d.ts",
      "import": "./dist/locales.js",
      "require": "./dist/locales.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-bundled": "tsc && vite build --config vite.config.bundled.ts && git checkout tmp-releases && rm -rf ../../release && mv ../../release-tmp ../../release",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings 0",
    "test": "vitest --run",
    "test-watch": "vitest watch",
    "clean": "rimraf dist && rimraf types"
  },
  "dependencies": {
    "@emoji-mart/data": "catalog:prod",
    "@shikijs/types": "catalog:syntax",
    "@tiptap/core": "catalog:prod",
    "@tiptap/extension-bold": "catalog:prod",
    "@tiptap/extension-code": "catalog:prod",
    "@tiptap/extension-gapcursor": "catalog:prod",
    "@tiptap/extension-history": "catalog:prod",
    "@tiptap/extension-horizontal-rule": "catalog:prod",
    "@tiptap/extension-italic": "catalog:prod",
    "@tiptap/extension-link": "catalog:prod",
    "@tiptap/extension-paragraph": "catalog:prod",
    "@tiptap/extension-strike": "catalog:prod",
    "@tiptap/extension-table-cell": "catalog:prod",
    "@tiptap/extension-table-header": "catalog:prod",
    "@tiptap/extension-text": "catalog:prod",
    "@tiptap/extension-underline": "catalog:prod",
    "@tiptap/pm": "catalog:prod",
    "emoji-mart": "catalog:prod",
    "hast-util-from-dom": "catalog:prod",
    "prosemirror-dropcursor": "catalog:prod",
    "prosemirror-highlight": "catalog:prod",
    "prosemirror-model": "catalog:prod",
    "prosemirror-state": "catalog:prod",
    "prosemirror-tables": "catalog:prod",
    "prosemirror-transform": "catalog:prod",
    "prosemirror-view": "catalog:prod",
    "rehype-format": "catalog:prod",
    "rehype-parse": "catalog:prod",
    "rehype-remark": "catalog:prod",
    "rehype-stringify": "catalog:prod",
    "remark-gfm": "catalog:prod",
    "remark-parse": "catalog:prod",
    "remark-rehype": "catalog:prod",
    "remark-stringify": "catalog:prod",
    "unified": "catalog:prod",
    "uuid": "catalog:prod",
    "y-prosemirror": "catalog:prod",
    "y-protocols": "catalog:prod",
    "yjs": "catalog:prod"
  },
  "devDependencies": {
    "@types/emoji-mart": "catalog:types",
    "@types/hast": "catalog:types",
    "@types/uuid": "catalog:types",
    "eslint": "catalog:lint",
    "jsdom": "catalog:prod",
    "rimraf": "catalog:node",
    "rollup-plugin-webpack-stats": "catalog:build",
    "typescript": "catalog:dev",
    "vite": "catalog:build",
    "vite-plugin-eslint": "catalog:lint",
    "vitest": "catalog:test"
  },
  "peerDependencies": {
    "@hocuspocus/provider": "catalog:peer"
  },
  "peerDependenciesMeta": {
    "@hocuspocus/provider": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.json"
    ]
  },
  "gitHead": "37614ab348dcc7faa830a9a88437b37197a2162d"
}
