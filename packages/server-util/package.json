    "y-protocols": "^1.0.6",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "eslint": "^8.57.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup-plugin-webpack-stats": "^0.2.6",
    "typescript": "^5.9.3",
    "vite": "^5.4.20",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0 || >= 19.0.0-rc",
    "react-dom": "^18.0 || ^19.0 || >= 19.0.0-rc"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.json"
    ]
