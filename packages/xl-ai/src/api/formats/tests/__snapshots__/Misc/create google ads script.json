                  "text": "function main() {
  var campaigns = AdsApp.campaigns().get();
  while (campaigns.hasNext()) {
    var campaign = campaigns.next();
    var budget = campaign.getBudget();
    var budgetAmount = budget.getAmount();
    var spend = campaign.getStatsFor('TODAY').getCost();
    if (spend >= budgetAmount) {
      sendEmailAlert(campaign.getName(), spend, budgetAmount);
    }
  }
}

function sendEmailAlert(campaignName, spend, budgetAmount) {
  var emailAddress = 'your-email@example.com';
  var subject = 'Budget Alert: ' + campaignName;
  var body = 'The campaign "' + campaignName + '" has spent $' + spend.toFixed(2) + ' out of its $' + budgetAmount.toFixed(2) + ' budget.';
  MailApp.sendEmail(emailAddress, subject, body);
