{
  "name": "@blocknote/xl-ai",
  "homepage": "https://github.com/TypeCellOS/BlockNote",
  "private": false,
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TypeCellOS/BlockNote.git",
    "directory": "packages/xl-ai"
  },
  "license": "AGPL-3.0 OR PROPRIETARY",
  "version": "0.35.0",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "keywords": [
    "ai",
    "llm",
    "react",
    "javascript",
    "editor",
    "typescript",
    "prosemirror",
    "wysiwyg",
    "rich-text-editor",
    "notion",
    "yjs",
    "block-based",
    "tiptap"
  ],
  "description": "A \"Notion-style\" block-based extensible text editor built on top of Prosemirror and Tiptap.",
  "type": "module",
  "source": "src/index.ts",
  "types": "./types/src/index.d.ts",
  "main": "./dist/blocknote-xl-ai.js",
  "module": "./dist/blocknote-xl-ai.js",
  "exports": {
    ".": {
      "types": "./types/src/index.d.ts",
      "import": "./dist/blocknote-xl-ai.js",
      "require": "./dist/blocknote-xl-ai.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "style": "./dist/style.css"
    },
    "./locales": {
      "types": "./types/src/i18n/locales/index.d.ts",
      "import": "./dist/locales.js",
      "require": "./dist/locales.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --build && vite build",
    "lint": "eslint src --max-warnings 0",
    "test": "NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\" vitest --run",
    "test-watch": "NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\" vitest watch",
    "email": "email dev"
  },
  "dependencies": {
    "@blocknote/core": "catalog:prod",
    "@blocknote/mantine": "catalog:prod",
    "@blocknote/prosemirror-suggest-changes": "catalog:prod",
    "@blocknote/react": "catalog:prod",
    "@floating-ui/react": "catalog:prod",
    "@tiptap/core": "catalog:prod",
    "ai": "catalog:prod",
    "lodash.isequal": "catalog:utils",
    "prosemirror-changeset": "catalog:prod",
    "prosemirror-model": "catalog:prod",
    "prosemirror-state": "catalog:prod",
    "prosemirror-tables": "catalog:prod",
    "prosemirror-transform": "catalog:prod",
    "prosemirror-view": "catalog:prod",
    "react": "catalog:prod",
    "react-dom": "catalog:prod",
    "react-icons": "catalog:icons",
    "remark-parse": "catalog:prod",
    "remark-stringify": "catalog:prod",
    "unified": "catalog:prod",
    "y-prosemirror": "catalog:prod",
    "zustand": "catalog:prod"
  },
  "devDependencies": {
    "@ai-sdk/groq": "catalog:prod",
    "@ai-sdk/mistral": "catalog:prod",
    "@ai-sdk/openai": "catalog:prod",
    "@ai-sdk/openai-compatible": "catalog:prod",
    "@ai-sdk/anthropic": "catalog:prod",
    "@ai-sdk/google": "catalog:prod",
    "@mswjs/interceptors": "catalog:dev",
    "@types/diff": "catalog:types",
    "@types/json-diff": "catalog:types",
    "@types/json-schema": "catalog:types",
    "@types/lodash.isequal": "catalog:types",
    "@types/react": "catalog:types",
    "@types/react-dom": "catalog:types",
    "@vitejs/plugin-react": "catalog:build",
    "@vitest/runner": "catalog:test",
    "eslint": "catalog:lint",
    "glob": "catalog:dev",
    "headers-polyfill": "catalog:dev",
    "msw": "catalog:dev",
    "msw-snapshot": "catalog:dev",
    "rimraf": "catalog:node",
    "rollup-plugin-webpack-stats": "catalog:build",
    "typescript": "catalog:dev",
    "undici": "catalog:dev",
    "vite": "catalog:build",
    "vite-plugin-eslint": "catalog:lint",
    "vite-plugin-externalize-deps": "catalog:build",
    "vitest": "catalog:test"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0 || >= 19.0.0-rc",
    "react-dom": "^18.0 || ^19.0 || >= 19.0.0-rc"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.json"
    ]
  }
}
